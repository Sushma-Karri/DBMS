-- 1Ô∏è‚É£ ROUND: Round number to nearest integer
SELECT ROUND(89.6, 0) AS Rounded_Value;

-- 2Ô∏è‚É£ CEIL / CEILING: Round up
SELECT CEIL(89.1) AS Ceil_Value;

-- 3Ô∏è‚É£ FLOOR: Round down
SELECT FLOOR(89.9) AS Floor_Value;

-- 4Ô∏è‚É£ ABS: Absolute value
SELECT ABS(-72) AS Absolute_Value;

-- 5Ô∏è‚É£ POWER / POW: Raise to a power
SELECT POWER(5, 3) AS Power_Value;  -- 5^3 = 125

-- 6Ô∏è‚É£ SQRT: Square root
SELECT SQRT(144) AS Square_Root;

-- 7Ô∏è‚É£ MOD: Modulus (remainder)
SELECT MOD(17, 5) AS Mod_Value;  -- 17 % 5 = 2

-- 8Ô∏è‚É£ EXP: Exponential e^x
SELECT EXP(2) AS Exponential_Value;

-- 9Ô∏è‚É£ LOG / LN: Natural logarithm
SELECT LOG(10) AS Log10_Value;   -- base 10
SELECT LN(2.71828) AS Ln_Value;  -- natural log

-- üîü TRUNCATE: Truncate decimal part
SELECT TRUNCATE(89.987, 1) AS Truncated_Value; -- 89.9
