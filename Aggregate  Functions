-- ðŸ”¹ Aggregate Functions Examples
-- 10ï¸âƒ£ Total students
SELECT COUNT(*) AS Total_Students FROM STUDENT;

-- 11ï¸âƒ£ Total marks
SELECT SUM(Marks) AS Total_Marks FROM STUDENT;

-- 12ï¸âƒ£ Average marks
SELECT AVG(Marks) AS Avg_Marks FROM STUDENT;

-- 13ï¸âƒ£ Highest marks
SELECT MAX(Marks) AS Highest_Marks FROM STUDENT;

-- 14ï¸âƒ£ Lowest marks
SELECT MIN(Marks) AS Lowest_Marks FROM STUDENT;

-- ðŸ”¹ GROUP BY Examples
-- 15ï¸âƒ£ Average marks per Department
SELECT Department, AVG(Marks) AS Avg_Marks
FROM STUDENT
GROUP BY Department;

-- 16ï¸âƒ£ Count of students per Department
SELECT Department, COUNT(*) AS Total_Students
FROM STUDENT
GROUP BY Department;

-- ðŸ”¹ HAVING Examples
-- 17ï¸âƒ£ Departments with average marks > 85
SELECT Department, AVG(Marks) AS Avg_Marks
FROM STUDENT
GROUP BY Department
HAVING AVG(Marks) > 85;

-- 18ï¸âƒ£ Departments with more than 1 student
SELECT Department, COUNT(*) AS Total_Students
FROM STUDENT
GROUP BY Department
HAVING COUNT(*) > 1;
