-- 🔹 Aggregate Functions Examples
-- 10️⃣ Total students
SELECT COUNT(*) AS Total_Students FROM STUDENT;

-- 11️⃣ Total marks
SELECT SUM(Marks) AS Total_Marks FROM STUDENT;

-- 12️⃣ Average marks
SELECT AVG(Marks) AS Avg_Marks FROM STUDENT;

-- 13️⃣ Highest marks
SELECT MAX(Marks) AS Highest_Marks FROM STUDENT;

-- 14️⃣ Lowest marks
SELECT MIN(Marks) AS Lowest_Marks FROM STUDENT;

-- 🔹 GROUP BY Examples
-- 15️⃣ Average marks per Department
SELECT Department, AVG(Marks) AS Avg_Marks
FROM STUDENT
GROUP BY Department;

-- 16️⃣ Count of students per Department
SELECT Department, COUNT(*) AS Total_Students
FROM STUDENT
GROUP BY Department;

-- 🔹 HAVING Examples
-- 17️⃣ Departments with average marks > 85
SELECT Department, AVG(Marks) AS Avg_Marks
FROM STUDENT
GROUP BY Department
HAVING AVG(Marks) > 85;

-- 18️⃣ Departments with more than 1 student
SELECT Department, COUNT(*) AS Total_Students
FROM STUDENT
GROUP BY Department
HAVING COUNT(*) > 1;
